pipeline {
    agent any
    stages {
        stage('fetch code') {
            steps {
                git branch: 'master', url:'https://github.com/bhaskar308/project.git'
            }
        }
        stage('build') {
            steps {
                sh'mvn install'
            }
        }
        stage('test') {
            steps {
                sh'mvn test'
            }
        }
    }
}
